/Write a C++ program for endl and setw function.

#include<iostream>
#include<iomanip>

using namespace std;

int main()
{
	cout<<"mahemdavadi riddhi j."<<endl;
	cout<<"220130318035"<<endl;
	int a;
	cout<<"Enter the value of a:";
	cin>>a;
	
	cout<<"The value of a is:"<<a<<endl;
	cout<<"The value of a is:"<<setw(5)<<a<<endl;
	return 0;
}
