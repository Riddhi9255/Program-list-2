//.Write a C++ program to add two integer values using call by reference.
#include <iostream>
using namespace std;

// Function to add two integers using call by reference
void add(int& a, int& b) {
    a += b;
}

int main() {
    int num1, num2;
     cout<<"mahemdavadi riddhi j"<<endl;
     cout<<"220130318035"<<endl;
    cout << "Enter the first integer: ";
    cin >> num1;

    cout << "Enter the second integer: ";
    cin >> num2;

    add(num1, num2); // Calling the add function with call by reference

    cout << "The sum of the two integers is: " << num1 << std::endl;

    return 0;
}
